{
  "name": "is-in-subnet",
  "version": "1.9.0",
  "description": "Check if an IPv4 or IPv6 address is contained in the given CIDR subnet",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "clean": "del-cli build",
    "build": "tsc",
    "browserify": "npm run clean && npm run build && browserify build/src/index.js -p tinyify -o browser/isInSubnet.js",
    "prepare": "npm run clean && npm run build && npm run browserify",
    "prettier": "prettier --write \"{src,test}/**/*.?(ts|json)\"",
    "lint": "eslint --ext .ts .",
    "test": "mocha \"./test/**/*.ts\"",
    "test:watch": "mocha \"./test/**/*.ts\"  -w --watch-extensions ts",
    "coverage": "nyc --reporter lcov --reporter text --extension .ts mocha \"./test/**/*.ts\""
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": "natesilva/is-in-subnet",
  "keywords": [
    "ip",
    "address",
    "ipv4",
    "ipv6",
    "cidr",
    "networking"
  ],
  "license": "MIT",
  "author": "Nate Silva <nate@natesilva.com>",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.18",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "browserify": "^16.5.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "tinyify": "^2.5.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  }
}
