{
  "name": "is-in-subnet",
  "version": "1.9.0",
  "description": "Check if an IPv4 or IPv6 address is contained in the given CIDR subnet",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "clean": "del-cli build .nyc_output coverage",
    "prepare": "del-cli build && tsc",
    "prettier": "prettier --write \"{src,test}/**/*.?(ts|json)\"",
    "test": "mocha \"./test/**/*.ts\"",
    "test:watch": "mocha \"./test/**/*.ts\"  -w --watch-extensions ts",
    "coverage": "nyc --reporter lcov --reporter text --extension .ts mocha \"./test/**/*.ts\""
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": "natesilva/is-in-subnet",
  "keywords": [
    "ip",
    "address",
    "ipv4",
    "ipv6",
    "cidr",
    "networking"
  ],
  "license": "MIT",
  "author": "Nate Silva <nate@natesilva.com>",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.18",
    "del-cli": "^3.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
